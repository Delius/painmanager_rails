<p id="notice"><%= notice %></p>

<h1>Listing Other Symptoms</h1>

<table>
  <thead>
    <tr>
      <th>Symotom name</th>
      <th>User</th>
      <th>Diary</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @other_symptoms.each do |other_symptom| %>
      <tr>
        <td><%= other_symptom.symotom_name %></td>
        <td><%= other_symptom.user %></td>
        <td><%= other_symptom.diary %></td>
        <td><%= link_to 'Show', other_symptom %></td>
        <td><%= link_to 'Edit', edit_other_symptom_path(other_symptom) %></td>
        <td><%= link_to 'Destroy', other_symptom, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Other symptom', new_other_symptom_path %>
