<p id="notice"><%= notice %></p>

<h1>Listing Effective Treatments</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>

    </tr>
  </thead>

  <tbody>
    <% @effective_treatments.each do |effective_treatment| %>
      <tr>
        <td><% effective_treatment.name.each do |name|%>
              <%= name %>
              <% end %>

              </td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

